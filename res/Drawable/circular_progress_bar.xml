<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="ring" android:useLevel="false" android:innerRadius="22.0dip" android:thickness="3.0dip">
            <solid android:color="@color/white" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <rotate android:fromDegrees="-90.0" android:toDegrees="-90.0" android:pivotX="50.0%" android:pivotY="50.0%">
            <shape android:shape="ring" android:useLevel="true" android:innerRadius="22.0dip" android:thickness="3.0dip">
                <solid android:color="@color/material_blue" />
            </shape>
        </rotate>
    </item>
</layer-list>